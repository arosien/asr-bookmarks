;(function($){
    $('.currencies li').bind('click', function(e) {
        $(this).parents('.currencies').find('li').removeClass('selected');
        $(this).addClass('selected');
    });

    $('.currencies li')
        .bind('mouseenter', function(){ $(this).addClass('hover'); })
        .bind('mouseleave', function(){ $(this).removeClass('hover'); });
})(jQuery);
