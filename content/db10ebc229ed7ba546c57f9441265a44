function checkForm(f) {
  var v=f.doc.value;
  if(v.indexOf(".")>-1) {
    location.href=f.getAttribute("action")+"../../view?doc="+v;
    //location.href=f.getAttribute("action")+"?doc="+v;
    //window.open("/docserver/view?doc="+v,"_blank");
   // f.setAttribute("method","get");
   // var n=f.newfile;
    //f.parentNode.removeChild(
    return false;
  } else
  return true;
}

</script>
