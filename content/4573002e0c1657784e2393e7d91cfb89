  <script type="text/javascript">
    $(document).observe('dom:loaded', function() {
        $$('ul#navigation li').each(function(li){
            li.observe('mouseover', function(){
                li.addClassName('hover');
            });
            li.observe('mouseout', function(){
                li.removeClassName('hover');
            });
        });
    });
  </script>
