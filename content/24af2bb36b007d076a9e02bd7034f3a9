<script>
var timer;
container = $('mainContent');
$('scrollDown').addEvents({
  mousedown: function(e)
  {
    timer = scrollContent.periodical(50, this,[container, false]);
  },
  mouseup: function()
  {
    if(timer)
      $clear(timer);
  }
});

$('scrollUp').addEvents({
  mousedown: function(e)
  {
    timer = scrollContent.periodical(50, this,[container, true]);
  },
  mouseup: function()
  {
    if(timer)
      $clear(timer);
  }
});

function scrollContent(ele, isUp)
{
  var pos = ele.getScroll().y;
  isUp ? ele.scrollTo(pos, pos - 5) : ele.scrollTo(pos, pos + 5);
}
  </script>
