  //<![CDATA[
    (new F1.Pane({
      id:"carousel_panes", 
      slide:'.slide', 
      scope: '#marketing',
      auto: 10000,
      after: function() {
        jq('.feature li').hide()
        jq('.feature li').eq(this.current_index).show()
        jq('.carousel_dots a').removeClass('on')
        jq('.carousel_dots a').eq(this.current_index).addClass('on')
      }
    })).init()
  //]]>
</script>
